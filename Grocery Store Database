(13, "flowers", 2, 57, 4),
(14, "soil", 100, 964, 2),
(15, "plants", 100, 2, 1);

SELECT * from grocery ORDER BY price asc;

SELECT sum(amount) from grocery;

SELECT avg(price) from grocery;


