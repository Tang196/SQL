CREATE TABLE grocery (id integer primary key, item TEXT, price INTEGER, amount INTEGER, aisle INTEGER);

INSERT INTO grocery VALUES
(1, "sage", 3, 100, 5),
(2, "mat", 1, 100, 4),
(3, "books", 10, 96, 3),
(4, "crystal", 5, 96, 3),
